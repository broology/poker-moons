{
  "version": 1,
  "projects": {
    "backend-feature-table": {
      "root": "libs/backend/feature/table",
      "sourceRoot": "libs/backend/feature/table/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "libs/backend/feature/table/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/backend/feature/table"
          ],
          "options": {
            "jestConfig": "libs/backend/feature/table/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "pokermoons-backend": {
      "root": "apps/pokermoons-backend",
      "sourceRoot": "apps/pokermoons-backend/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/pokermoons-backend",
            "main": "apps/pokermoons-backend/src/main.ts",
            "tsConfig": "apps/pokermoons-backend/tsconfig.app.json",
            "assets": [
              "apps/pokermoons-backend/src/assets"
            ],
            "maxWorkers": 2,
            "memoryLimit": 1024
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/pokermoons-backend/src/environments/environment.ts",
                  "with": "apps/pokermoons-backend/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "pokermoons-backend:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/pokermoons-backend/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/pokermoons-backend"
          ],
          "options": {
            "jestConfig": "apps/pokermoons-backend/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "pokermoons-frontend": {
      "projectType": "application",
      "root": "apps/pokermoons-frontend",
      "sourceRoot": "apps/pokermoons-frontend/src",
      "prefix": "poker-moons",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/pokermoons-frontend",
            "index": "apps/pokermoons-frontend/src/index.html",
            "main": "apps/pokermoons-frontend/src/main.ts",
            "polyfills": "apps/pokermoons-frontend/src/polyfills.ts",
            "tsConfig": "apps/pokermoons-frontend/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/pokermoons-frontend/src/favicon.ico",
              "apps/pokermoons-frontend/src/assets"
            ],
            "styles": [
              "apps/pokermoons-frontend/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/pokermoons-frontend/src/environments/environment.ts",
                  "with": "apps/pokermoons-frontend/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "pokermoons-frontend:build:production"
            },
            "development": {
              "browserTarget": "pokermoons-frontend:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "pokermoons-frontend:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/pokermoons-frontend/src/**/*.ts",
              "apps/pokermoons-frontend/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/pokermoons-frontend"
          ],
          "options": {
            "jestConfig": "apps/pokermoons-frontend/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "pokermoons-frontend-e2e": {
      "root": "apps/pokermoons-frontend-e2e",
      "sourceRoot": "apps/pokermoons-frontend-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/pokermoons-frontend-e2e/cypress.json",
            "devServerTarget": "pokermoons-frontend:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "pokermoons-frontend:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/pokermoons-frontend-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "shared-type": {
      "root": "libs/shared/type",
      "sourceRoot": "libs/shared/type/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "libs/shared/type/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/shared/type"
          ],
          "options": {
            "jestConfig": "libs/shared/type/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "packageManager": "npm"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  }
}
