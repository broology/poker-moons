{
    "version": 1,
    "projects": {
        "backend-feature-table": {
            "root": "libs/backend/feature/table",
            "sourceRoot": "libs/backend/feature/table/src",
            "projectType": "library",
            "architect": {
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "outputs": ["{options.outputFile}"],
                    "options": {
                        "lintFilePatterns": ["libs/backend/feature/table/**/*.ts"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": ["coverage/libs/backend/feature/table"],
                    "options": {
                        "jestConfig": "libs/backend/feature/table/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "shared-type": {
            "root": "libs/shared/type",
            "sourceRoot": "libs/shared/type/src",
            "projectType": "library",
            "architect": {
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "outputs": ["{options.outputFile}"],
                    "options": {
                        "lintFilePatterns": ["libs/shared/type/**/*.ts"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": ["coverage/libs/shared/type"],
                    "options": {
                        "jestConfig": "libs/shared/type/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        }
    },
    "cli": {
        "defaultCollection": "@nrwl/angular",
        "packageManager": "npm"
    },
    "schematics": {
        "@nrwl/angular": {
            "application": {
                "linter": "eslint"
            },
            "library": {
                "linter": "eslint"
            },
            "storybook-configuration": {
                "linter": "eslint"
            }
        },
        "@nrwl/angular:application": {
            "style": "scss",
            "linter": "eslint",
            "unitTestRunner": "jest",
            "e2eTestRunner": "cypress"
        },
        "@nrwl/angular:library": {
            "style": "scss",
            "linter": "eslint",
            "unitTestRunner": "jest"
        },
        "@nrwl/angular:component": {
            "style": "scss"
        }
    }
}
